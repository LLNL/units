{
  "line_width": 88,
  "dangle_parens": true,
  "command_case": "lower",
  "separate_fn_name_with_space": false,
  "always_wrap": [],
  "separate_ctrl_name_with_space": false,
  "max_lines_hwrap": 2,
  "tab_size": 4,
  "max_subgroups_hwrap": 2,
  "max_pargs_hwrap": 6,
  "keyword_case": "upper",
  "enum_char": ".",
  "bullet_char": "*",
  "line_ending": "unix",
  "first_comment_is_literal": true,
  "literal_comment_pattern": "Copyright"
}
